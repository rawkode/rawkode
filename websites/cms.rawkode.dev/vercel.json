{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "use": "@vercel/static-build",
      "src": "package.json",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
