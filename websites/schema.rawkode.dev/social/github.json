{
  "$id": "https://schema.rawkode.dev/social/github",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Represents a GitHub account",
  "type": "object",
  "required": [
    "username"
  ],
  "properties": {
    "username": {
      "type": "string",
      "description": "The GitHub username",
      "pattern": "^@[a-zA-Z0-9_-]{1,39}$"
    }
  }
}
