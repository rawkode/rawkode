{
	"name": "@rawkode/dhd",
	"version": "0.1.0",
	"description": "DHD - Dial Home Device - Configuration management framework",
	"main": "index.ts",
	"bin": {
		"dhd": "./cli.ts"
	},
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": "./index.ts",
		"./cli": "./cli.ts",
		"./core": "./core/index.ts",
		"./core/*.ts": "./core/*.ts",
		"./core/actions": "./core/actions/index.ts",
		"./core/actions/*.ts": "./core/actions/*.ts",
		"./core/build": "./core/build/index.ts",
		"./core/build/*.ts": "./core/build/*.ts",
		"./core/ink": "./core/ink/index.ts",
		"./core/ink/*.tsx": "./core/ink/*.tsx",
		"./utils": "./utils/index.ts",
		"./utils/*.ts": "./utils/*.ts",
		"./utils/*/mod.ts": "./utils/*/mod.ts"
	},
	"scripts": {
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@effect/platform": "^0.77.6",
		"@effect/platform-node": "^0.73.0",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/api-logs": "^0.201.1",
		"@types/react": "^18.3.17",
		"chalk": "^5.4.1",
		"effect": "3.12.5",
		"fs-extra": "^11.3.0",
		"glob": "^11.0.0",
		"ink": "^5.0.1",
		"meow": "^13.2.0",
		"react": "^18.3.1",
		"react-devtools-core": "^6.1.2"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^22.10.5",
		"typescript": "^5.7.3"
	}
}
